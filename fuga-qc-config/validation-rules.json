    "genre": {
      "minCount": 1,
      "maxCount": 3,
      "requireSelection": true,
      "preventMismatch": true,
      "classicalRequiresClassicalFormat": true
    },
    
    "isrc": {
      "format": "CCXXXYYNNNNNNN",
      "example": "USKRE2400001",
      "required": true,
      "preventDuplicates": true,
      "validateCountryCode": true,
      "validateYear": true
    },
    
    "upc": {
      "format": "12-14 digits",
      "example": "123456789012",
      "required": true,
      "validateChecksum": true,
      "preventDuplicates": true
    },
    
    "copyright": {
      "requireHolder": true,
      "requireYear": true,
      "format": "{year} {holder}",
      "preventSymbols": true,
      "validateYear": true
    },
    
    "credits": {
      "composer": {
        "requireFullName": true,
        "minimumParts": 2,
        "preventAbbreviations": true,
        "preventPseudonyms": true,
        "format": "FirstName LastName"
      },
      "lyricist": {
        "requireFullName": true,
        "minimumParts": 2,
        "preventAbbreviations": true,
        "preventPseudonyms": true,
        "format": "FirstName LastName"
      }
    },
    
    "featuring": {
      "format": "feat.",
      "position": "afterTitle",
      "separator": " ",
      "preventInArtistName": true,
      "consistentFormat": true
    },
    
    "parentalAdvisory": {
      "autoDetect": true,
      "requireForExplicitContent": true,
      "explicitRequiresCleanVersion": false,
      "checkLyrics": true
    },
    
    "audio": {
      "requireLossless": true,
      "minSampleRate": 44100,
      "minBitDepth": 16,
      "requireStereo": true,
      "preventClipping": true,
      "requireProperLoudness": true
    },
    
    "artwork": {
      "minResolution": 3000,
      "maxResolution": 6000,
      "aspectRatio": "1:1",
      "format": ["JPG", "PNG"],
      "colorSpace": "RGB",
      "maxFileSizeMB": 50,
      "preventText": true,
      "preventURLs": true,
      "requireHighQuality": true
    },
    
    "video": {
      "minResolution": "1280x720",
      "requireHD": true,
      "preventLetterboxing": true,
      "requireProperAspectRatio": true,
      "preventWatermarks": true,
      "requireClosedCaptions": false
    }
  },
  
