generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type SavedArtistIdentifiers {
  type  String
  /// Could not determine type: the field only had null or empty values in the sample set.
  url   Json?
  value String
}

type SavedArtistTranslations {
  language String
  name     String
}

type SavedContributorTranslations {
  language String
  name     String
}

type SubmissionFiles {
  coverImageUrl           String?
  artistPhotoUrl          String?
  motionArtUrl            String?
  musicVideoUrl           String?
  audioFiles              AudioFile[]
  additionalFiles         AdditionalFile[]
  musicVideoFiles         MusicVideoFile[]
  musicVideoThumbnails    MusicVideoThumbnail[]
}

type AudioFile {
  trackId     String?
  dropboxUrl  String
  fileName    String?
  fileSize    Int?
}

type AdditionalFile {
  dropboxUrl  String
  fileName    String?
  fileType    String?
  fileSize    Int?
}

type MusicVideoFile {
  trackId     String?
  dropboxUrl  String
  fileName    String?
}

type MusicVideoThumbnail {
  trackId     String?
  dropboxUrl  String
  fileName    String?
}

type SubmissionRelease {
  artistName            String
  cRights               String
  consumerReleaseDate   String
  consumerReleaseUTC    DateTime @db.Date
  copyrightHolder       String
  copyrightYear         String
  /// Could not determine type: the field only had null or empty values in the sample set.
  distributors          Json?
  hasSyncHistory        Boolean
  /// Could not determine type: the field only had null or empty values in the sample set.
  instruments           Json?
  isCompilation         Boolean
  /// Could not determine type: the field only had null or empty values in the sample set.
  moods                 Json?
  motionArtwork         Boolean
  originalReleaseDate   String
  originalReleaseUTC    DateTime @db.Date
  pRights               String
  parentalAdvisory      String
  preOrderEnabled       Boolean
  previouslyReleased    Boolean
  priceType             String
  recordingCountry      String
  recordingLanguage     String
  releaseFormat         String
  releaseTime           String
  releaseUTC            DateTime @db.Date
  selectedTimezone      String
  /// Could not determine type: the field only had null or empty values in the sample set.
  territories           Json?
  territoryType         String
  thisIsPlaylist        Boolean
  upc                   String
  youtubeShortsPreviews Boolean
}

type SubmissionTracks {
  /// Could not determine type: the field only had null or empty values in the sample set.
  artists          Json?
  composer         String
  /// Could not determine type: the field only had null or empty values in the sample set.
  contributors     Json?
  dolbyAtmos       Boolean
  explicitContent  Boolean
  /// Could not determine type: the field only had null or empty values in the sample set.
  featuringArtists Json?
  id               String
  isFocusTrack     Boolean
  isTitle          Boolean
  isrc             String
  lyricist         String
  stereo           Boolean
  titleEn          String
  titleKo          String
  trackType        String
  versionType      String
}

type UserPreferences {
  language      String
  notifications UserPreferencesNotifications
}

type UserPreferencesNotifications {
  email Boolean
  push  Boolean
  sms   Boolean
}

model SavedArtist {
  id              String                    @id @default(auto()) @map("_id") @db.ObjectId
  completionScore BigInt
  createdAt       DateTime                  @db.Date
  /// Could not determine type: the field only had null or empty values in the sample set.
  dspProfiles     Json?
  identifiers     SavedArtistIdentifiers[]
  lastUsed        DateTime                  @db.Date
  /// Could not determine type: the field only had null or empty values in the sample set.
  missingFields   Json?
  name            String
  releaseCount    BigInt
  /// Could not determine type: the field only had null or empty values in the sample set.
  similarArtists  Json?
  /// Could not determine type: the field only had null or empty values in the sample set.
  socialProfiles  Json?
  status          String
  translations    SavedArtistTranslations[]
  updatedAt       DateTime                  @db.Date
  usageCount      BigInt
  userId          String                    @db.ObjectId
}

model SavedContributor {
  id           String                         @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime                       @db.Date
  /// Could not determine type: the field only had null or empty values in the sample set.
  identifiers  Json?
  instruments  String[]
  lastUsed     DateTime                       @db.Date
  name         String
  roles        String[]
  translations SavedContributorTranslations[]
  usageCount   BigInt
  userId       String                         @db.ObjectId
}

model Submission {
  id             String             @id @default(auto()) @map("_id") @db.ObjectId
  /// Could not determine type: the field only had null or empty values in the sample set.
  albumGenre     Json?
  /// Could not determine type: the field only had null or empty values in the sample set.
  albumSubgenre  Json?
  albumTitle     String
  albumTitleEn   String
  albumType      String
  artistName     String
  artistNameEn   String
  createdAt      DateTime           @db.Date
  files          SubmissionFiles
  /// Could not determine type: the field only had null or empty values in the sample set.
  genre          Json?
  labelName      String
  release        SubmissionRelease
  releaseDate    DateTime           @db.Date
  status         String
  submitterEmail String
  submitterId    String             @db.ObjectId
  submitterName  String
  tracks         SubmissionTracks[]
  updatedAt      DateTime           @db.Date
}

model User {
  id                String          @id @default(auto()) @map("_id") @db.ObjectId
  company           String?
  createdAt         DateTime        @db.Date
  email             String          @unique
  emailVerified     Boolean
  googleId          String          @unique
  isActive          Boolean
  isCompanyAccount  Boolean?
  isProfileComplete Boolean
  lastLogin         DateTime?       @db.Date
  name              String
  phone             String?
  preferences       UserPreferences
  profilePicture    String
  provider          String
  role              String
  updatedAt         DateTime        @db.Date
}
